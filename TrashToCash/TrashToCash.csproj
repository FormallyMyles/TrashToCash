<Project Sdk="Microsoft.NET.Sdk">

	<PropertyGroup>
		<TargetFramework>net472</TargetFramework>
		<Authors>FormallyMyles</Authors>
		<Company>FormallyMyles</Company>
		<AssemblyVersion>1.1.0</AssemblyVersion>
		<FileVersion>1.1.0</FileVersion>
	</PropertyGroup>

	<PropertyGroup>
		<!-- Your plateup game folder goes here -->
		<PlateUpFolder>G:\SteamLibrary\steamapps\common\PlateUp\PlateUp</PlateUpFolder>
	</PropertyGroup>
	
	<ItemGroup>
